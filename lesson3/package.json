{
  "name": "openfin_lesson3",
  "version": "1.0.0",
  "description": "Learning openfin with react",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all build:dev --parallel start:dev launchopenfin",
    "launchopenfin": "wait-on http://localhost:3000 && openfin --launch --config http://localhost:3000/app.json",
    "start:dev": "babel-watch ./webpack/server.js",
    "build:dev": "rimraf build && cross-env NODE_ENV=production webpack --config webpack/dev.webpack.js",
    "start:prod": "cross-env serve -s ./build/",
    "build:prod": "rimraf build && cross-env NODE_ENV=production webpack --config webpack/prod.webpack --progress",
    "lint": "eslint src/**/*.{ts,tsx}",
    "lint-fix": "eslint **/*/*.tsx  --fix"
  },
  "keywords": [
    "react",
    "typescript",
    "openfin"
  ],
  "dependencies": {
